{
  "name": "shapekit",
  "version": "0.1.1",
  "type": "module",
  "main": "dist/core.js",
  "module": "dist/core.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/core.cjs"
    },
    "./bounds": {
      "import": "./dist/bounds.js",
      "types": "./dist/types/bounds/index.d.ts",
      "require": "./dist/bounds.cjs"
    },
    "./json": {
      "import": "./dist/json.js",
      "types": "./dist/types/json/index.d.ts",
      "require": "./dist/json.cjs"
    },
    "./transforms": {
      "import": "./dist/transforms.js",
      "types": "./dist/types/transforms/index.d.ts",
      "require": "./dist/transforms.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "canvas": "^3.1.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.3"
  }
}
